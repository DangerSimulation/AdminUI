{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
	"AdminUi": {
	  "projectType": "application",
	  "i18n": {
		"sourceLocale": "en-US",
		"locales": {
		  "de": "src/locales/messages.de.xlf"
		}
	  },
	  "schematics": {
		"@schematics/angular:application": {
		  "strict": true
		}
	  },
	  "root": "",
	  "sourceRoot": "src",
	  "prefix": "app",
	  "architect": {
		"build": {
		  "builder": "@angular-devkit/build-angular:browser",
		  "options": {
			"outputPath": "dist/AdminUi",
			"index": "src/index.html",
			"main": "src/main.ts",
			"polyfills": "src/polyfills.ts",
			"tsConfig": "tsconfig.app.json",
			"aot": true,
			"assets": [
			  "src/favicon.ico",
			  "src/assets"
			],
			"styles": [
			  "./node_modules/@nebular/theme/styles/prebuilt/cosmic.css",
			  "src/styles.css"
			],
			"scripts": []
		  },
		  "configurations": {
			"production": {
			  "fileReplacements": [
				{
				  "replace": "src/environments/environment.ts",
				  "with": "src/environments/environment.prod.ts"
				}
			  ],
			  "optimization": true,
			  "outputHashing": "all",
			  "sourceMap": false,
			  "namedChunks": false,
			  "extractLicenses": true,
			  "vendorChunk": false,
			  "buildOptimizer": true,
			  "budgets": [
				{
				  "type": "initial",
				  "maximumWarning": "5mb",
				  "maximumError": "10mb"
				},
				{
				  "type": "anyComponentStyle",
				  "maximumWarning": "2kb",
				  "maximumError": "4kb"
				}
			  ]
			},
			"web": {
			  "localize": [
				"de"
			  ],
			  "optimization": false,
			  "outputHashing": "all",
			  "sourceMap": true,
			  "namedChunks": false,
			  "aot": true,
			  "extractLicenses": true,
			  "vendorChunk": false,
			  "buildOptimizer": false,
			  "fileReplacements": [
				{
				  "replace": "src/environments/environment.ts",
				  "with": "src/environments/environment.web.ts"
				}
			  ]
			}
		  }
		},
		"serve": {
		  "builder": "@angular-devkit/build-angular:dev-server",
		  "options": {
			"browserTarget": "AdminUi:build"
		  },
		  "configurations": {
			"production": {
			  "browserTarget": "AdminUi:build:production"
			},
			"web": {
			  "browserTarget": "AdminUi:build:web"
			}
		  }
		},
		"extract-i18n": {
		  "builder": "@angular-devkit/build-angular:extract-i18n",
		  "options": {
			"browserTarget": "AdminUi:build"
		  }
		}
	  }
	}
  },
  "defaultProject": "AdminUi"
}

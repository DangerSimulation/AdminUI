{
  "name": "admin-ui",
  "version": "0.0.0",
  "scripts": {
	"postinstall": "electron-builder install-app-deps",
	"ng": "ng",
	"start": "npm-run-all -p electron:serve ng:serve",
	"forgeStart": "electron-forge start",
	"build": "npm run electron:serve-tsc && ng build --base-href ./",
	"build:dev": "npm run build -- -c dev",
	"build:prod": "npm run build -- -c production",
	"ng:serve": "ng serve -c web",
	"electron:serve-tsc": "tsc -p tsconfig.app.json",
	"electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
	"electron:local": "npm run build:prod && npx electron .",
	"electron:build": "npm run build:prod && electron-builder build --publish=never",
	"lint": "ng lint",
	"package": "electron-forge package",
	"make": "electron-forge make"
  },
  "main": "./main.js",
  "private": true,
  "dependencies": {
	"@angular/animations": "~11.2.3",
	"@angular/cdk": "^11.0.0",
	"@angular/common": "~11.2.3",
	"@angular/compiler": "~11.2.3",
	"@angular/core": "~11.2.3",
	"@angular/forms": "~11.2.3",
	"@angular/platform-browser": "~11.2.3",
	"@angular/platform-browser-dynamic": "~11.2.3",
	"@angular/router": "~11.2.3",
	"@electron/remote": "^1.0.4",
	"@nebular/eva-icons": "7.0.0",
	"@nebular/theme": "^7.0.0",
	"@types/ws": "^7.4.0",
	"electron-squirrel-startup": "^1.0.0",
	"eva-icons": "^1.1.2",
	"ngx-electron": "^2.2.0",
	"node": "^15.10.0",
	"rxjs": "~6.6.0",
	"tslib": "^2.0.0",
	"ws": "^7.4.3",
	"zone.js": "~0.11.3"
  },
  "devDependencies": {
	"@angular-devkit/build-angular": "~0.1102.2",
	"@angular/cli": "~11.2.2",
	"@angular/compiler-cli": "~11.2.3",
	"@angular/localize": "^11.2.3",
	"@electron-forge/cli": "^6.0.0-beta.54",
	"@electron-forge/maker-deb": "^6.0.0-beta.54",
	"@electron-forge/maker-rpm": "^6.0.0-beta.54",
	"@electron-forge/maker-squirrel": "^6.0.0-beta.54",
	"@electron-forge/maker-zip": "^6.0.0-beta.54",
	"@schematics/angular": "~11.2.2",
	"@types/jasmine": "~3.6.0",
	"@types/node": "14.14.31",
	"codelyzer": "^6.0.0",
	"electron": "^12.0.0",
	"electron-reload": "^1.5.0",
	"jasmine-core": "~3.6.0",
	"jasmine-spec-reporter": "~5.0.0",
	"karma": "~6.1.0",
	"karma-chrome-launcher": "~3.1.0",
	"karma-coverage": "~2.0.3",
	"karma-jasmine": "~4.0.0",
	"karma-jasmine-html-reporter": "^1.5.0",
	"npm-run-all": "^4.1.5",
	"protractor": "~7.0.0",
	"ts-node": "~8.3.0",
	"typescript": "~4.1.2",
	"wait-on": "^5.2.1"
  },
  "config": {
	"forge": {
	  "packagerConfig": {},
	  "makers": [
		{
		  "name": "@electron-forge/maker-squirrel",
		  "config": {
			"name": "admin_ui",
			"author": "DangerSimulation",
			"description": "The admin view for the simulation"
		  }
		},
		{
		  "name": "@electron-forge/maker-zip",
		  "platforms": [
			"darwin"
		  ]
		},
		{
		  "name": "@electron-forge/maker-deb",
		  "config": {}
		},
		{
		  "name": "@electron-forge/maker-rpm",
		  "config": {}
		}
	  ]
	}
  }
}
